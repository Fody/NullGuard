    <PackageReference Include="xunit.runner.visualstudio" Version="2.4.1" />
    <Reference Include="Microsoft.CSharp" />
    <ProjectReference Include="..\AssemblyToProcessExplicit\AssemblyToProcessExplicit.csproj" />
